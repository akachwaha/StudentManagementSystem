@model IEnumerable<SMS_3.Models.Tutor>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add Tutor", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Tutor Name")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tutoremail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tutorname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tutoremail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TutorId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TutorId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TutorId })
        </td>
    </tr>
}

</table>
